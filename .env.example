# Push Notifications Configuration
# Copy this file to .env.local and fill in your actual values

# VAPID keys for push notifications
# Generate new keys with: npx web-push generate-vapid-keys
VITE_VAPID_PUBLIC_KEY=BK4jK...YOUR_PUBLIC_KEY_HERE
VAPID_PRIVATE_KEY=Lm8f...YOUR_PRIVATE_KEY_HERE

# Push server endpoint (optional - for custom push server)
VITE_PUSH_ENDPOINT=https://your-server.com/api/push

# App configuration
VITE_APP_NAME="Push-Up Counter"
VITE_APP_VERSION=1.0.0

# Development settings
NODE_ENV=development
